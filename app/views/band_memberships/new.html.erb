<form action="<%= band_memberships_path %>" method="post">
  <input type="text" name="instrument">
  <select name="musician_id" >
    <% @musicians.each do |musician| %> 
      <option value="<%=musician.id%>"><%= musician.name %></option>
    <% end %>
  </select>
  <select name="band_id" >
    <% @bands.each do |band| %> 
      <option value="<%=band.id %>"><%= band.name %></option>
    <% end %>
  </select>
  <button id="submit">Create Band Membership!</button>
</form>